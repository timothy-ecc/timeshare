<div id="<%= dom_id report %>">


  <!-- Date and rating section -->
  <div class="container">
    <div class="row week-ending-row">
      <div class="col-6">
        <h4>Week ending:  <%= report.week_ending.strftime("%m/%d/%Y") %></h4>
      </div>
      <div class="col-6">
        <h4>Rating: <%= define_rating(report.rating) %></h4>
      </div>
    </div>
  </div> <!-- end date and rating section -->

  <div class="container report-header">
    <h4>Accomplishments/Tasks Completed This Week</h4>
    <%= report.completed %>
  </div>


  <div class="container report-header">
    <h4>Upcoming Tasks Next Week</h4>
    <%= report.upcoming %>
  </div>
  <div class="container report-header">
    <h4>Notes</h4>
    <%= report.notes %>
  </div>
</div>
</div>

